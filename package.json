{
  "name": "contactbook",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.5.0",
    "@sap/cds": "^7",
    "express": "^4",
    "sap-cf-axios": "^0.4.10"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/ux-specification": "^1.108.14"
  },
  "scripts": {
    "start": "cds-serve",
    "backend": "cds watch --profile backend",
    "watch-contactbook": "cds watch --open contactbook/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "contactset": {
        "kind": "odata-v2",
        "model": "srv/external/contactset",
        "[backend]": {
          "credentials": {
            "destination": "contacts"
          }
        }
      }
    }
  },
  "sapux": [
    "app/contactbook"
  ]
}
